{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"way/generators": "~2.0",
		"laravelbook/ardent": "dev-master",
		"anahkiasen/former": "dev-master",
		"stevemo/cpanel": "dev-develop",
		"intervention/image": "dev-master",
		"torann/currency": "dev-master",
		"raveren/kint": "dev-master",
		"barryvdh/laravel-debugbar": "1.*",
		"zizaco/confide": "~4.0@dev",
		"zizaco/entrust": "1.2.1",
		"fitztrev/laravel-html-minify": "1.*",
		"laracasts/presenter": "0.1.*",
		"laracasts/validation": "~1.0",
		"mews/purifier": "1.0.2",
		"rcrowe/turbo": "0.2.*",
		"phpunit/phpunit": "~4.4.0@stable",
		"codeception/codeception": "*",
        "nickcernis/html-to-markdown": "2.1.*",
        "erusev/parsedown": "~1.1",
        "laracasts/flash": "~1.0",
        "artdarek/oauth-4-laravel": "dev-master",
        "thujohn/rss": "~1.0",
        "pusher/pusher-php-server": "dev-master",
        "codesleeve/asset-pipeline": "dev-master",
		"barryvdh/laravel-ide-helper": "~1.11",
		"lucadegasperi/oauth2-server-laravel": "3.*",
		"dingo/api": "0.8.*"
	},
	"require-dev": {
		"fzaninotto/faker": "1.4.*",
		"mockery/mockery": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "Gtskk\\": "app/Gtskk"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
